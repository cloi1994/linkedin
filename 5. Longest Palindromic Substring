class Solution(object):
    def longestPalindrome(self, s):
        """
        :type s: str
        :rtype: str
        """
        
        if not s or len(s) <= 1:
            return s
        
        
        dp = [ [ False for i in range(len(s))] for j in range(len(s))]
        
        maxLen = 1
        longest = ""
        
        for i in range(len(s)-1,-1,-1):
             for j in range(i,len(s)):
                    
                    
                if s[i] == s[j] and (j - i <= 2 or dp[i + 1][j - 1]):
                    
                    dp[i][j] = True
                        
                    if j - i + 1 > maxLen:
                        maxLen = j - i + 1;
                        longest = s[i:j + 1]
                        
        return longest
